{
	"welcomeFile": "index.html",
	"logout": {
		"logoutPage": "/logout.html",
		"logoutEndpoint": "/my/logout"
	},
	"websockets": {
		"enabled": true
	},
	"authenticationMethod": "route",
	"routes": [{
		"source": "/catalog/(.*)",
		"destination": "srv_api",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}, {
		"source": "/odata/v4/(.*)",
		"destination": "srv_api",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}, {
		"source": "/node(.*)",
		"destination": "srv_api",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsjs)",
		"destination": "xsjs_api",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "xsjs_api",
		"authenticationType": "xsuaa"
	},
	{
		"source": "/(.*)",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"replace": {
			"pathSuffixes": [
				"index.html",
				"odataTest.html"
			],
			"services": {
				"sapui5_sb": {
					"tag": "ui5"
				}
			}
		}
	}]
}